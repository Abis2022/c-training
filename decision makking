#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int c;
    if(n<=200)
    {
         c=n*0.5;
        //printf("%d",c);
    }
    else if((n>200)&&(n<=400)){
        c=(n*0.65)+100;
       // printf("%d",d);
    }
    else if((n>400)&&(n<=600)){
        c=(n*0.80)+200;
        //printf("%d",f);
    }
    else {
        c=(n*1.25)+425;
         //printf("%d",e);
    }
    printf("%d",c);
return 0;
}

7)#include<stdio.h>
int main()
{
    int m,r,d;
    scanf("%d%d%d",&m,&r,&d);
    if((m>0)&&(m<=12)){
        if((m>3)&&(m<=5)||(m==11)||(m==12)))
        {
            int rent=(r+(r*20/100))*d;
              printf("%d",rent);
        }
        else{
            int rent=(r*d);
            
        }
    }
    else{
        printf("Invalid month");
    }
  
}
