2)// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int a,s,c;
    scanf("%d%d%d",&a,&s,&c);
    if((a>=21)&&(a<=60))
    {
        if((c>=750)&&(c<=850)&&(s>=300000)){
            printf("Excellent-Eligible ");
        }
        else if((c>=700)&&(c<=749)&&(s>=300000)){
            printf("Good-Eligible ");
        }
        else if((c>=650)&&(c<=699)&&(s>=300000)){
            printf("Fair-Eligible ");
        }
        else if((c>=550)&&(c<=649)&&(s>=300000)){
            printf("Poor-Eligible ");
        }
        else{
            printf("Not Eligible");
        }
    }

    return 0;

3)// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int n,m;
    scanf("%d",&n);
    int t=0;
    while(n>0){
        m=n%10;
        t=t*10+m;
        n=n/10;
    }
    printf("%d",t);
    return 0;
}


4)// Online C compiler to run C program online
#include <stdio.h>

// Online C compiler to run C program online
#include <stdio.h>

int main() {
    char c;
    scanf("%c",&c);
    if((c=='a')||(c=='e')||(c=='i')||(c=='o')||(c=='u')||(c=='A')||(c=='E')||(c=='I')||(c=='O')||(c=='U'))
    {
    printf("Vowels");
    }
    else if(c>='0' && c<='9'){
    printf("Digits");
    } 
    else{
        printf("consonants");
    }
return 0;
}

